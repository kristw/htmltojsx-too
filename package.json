{
  "version": "1.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "html",
    "jsx",
    "converter",
    "typescript",
    "react"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
